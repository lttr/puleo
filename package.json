{
  "type": "module",
  "scripts": {
    "build": "node src/build.js",
    "build:examples": "postcss examples/css-features.css -o examples/css-features.min.css",
    "dev": "run-p watch start",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "browser-sync start --server --files . --no-notify",
    "watch": "nodemon --ext js --exec npm run build"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^2.1.0",
    "browser-sync": "^2.29.3",
    "cssnano": "^6.0.1",
    "defu": "^6.1.3",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-dark-theme-class": "^1.0.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "open-props": "^1.6.10"
  }
}
